    <script>
        function showTab(tabName) {
            // Hide all tabs
            document.querySelectorAll('.tab-content').forEach(tab => {
                tab.classList.remove('active');
            });
            
            // Remove active from all buttons
            document.querySelectorAll('.tab-button').forEach(btn => {
                btn.classList.remove('active');
            });
            
            // Show selected tab
            document.getElementById(tabName).classList.add('active');
            
            // Update header based on tab
            const headers = {
                tradition: { icon: 'üéä', title: '‡∏õ‡∏£‡∏∞‡πÄ‡∏û‡∏ì‡∏µ‡πÅ‡∏•‡∏∞‡πÄ‡∏ó‡∏®‡∏Å‡∏≤‡∏•', subtitle: '‡∏á‡∏≤‡∏ô‡∏ö‡∏∏‡∏ç ‡∏û‡∏¥‡∏ò‡∏µ‡∏Å‡∏£‡∏£‡∏° ‡πÄ‡∏ó‡∏®‡∏Å‡∏≤‡∏•‡∏ó‡πâ‡∏≠‡∏á‡∏ñ‡∏¥‡πà‡∏ô' },
                craft: { icon: 'üé®', title: '‡∏®‡∏¥‡∏•‡∏õ‡∏∞‡πÅ‡∏•‡∏∞‡∏´‡∏±‡∏ï‡∏ñ‡∏Å‡∏£‡∏£‡∏°', subtitle: '‡∏á‡∏≤‡∏ô‡∏ù‡∏µ‡∏°‡∏∑‡∏≠ ‡∏®‡∏¥‡∏•‡∏õ‡∏∞‡∏û‡∏∑‡πâ‡∏ô‡∏ö‡πâ‡∏≤‡∏ô' },
                food: { icon: 'üçú', title: '‡∏≠‡∏≤‡∏´‡∏≤‡∏£‡πÅ‡∏•‡∏∞‡∏ß‡∏±‡∏ï‡∏ñ‡∏∏‡∏î‡∏¥‡∏ö‡∏ó‡πâ‡∏≠‡∏á‡∏ñ‡∏¥‡πà‡∏ô', subtitle: '‡∏ï‡∏≥‡∏£‡∏±‡∏ö‡∏≠‡∏≤‡∏´‡∏≤‡∏£ ‡∏†‡∏π‡∏°‡∏¥‡∏õ‡∏±‡∏ç‡∏ç‡∏≤‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏≠‡∏≤‡∏´‡∏≤‡∏£' },
                history: { icon: 'üìú', title: '‡∏õ‡∏£‡∏∞‡∏ß‡∏±‡∏ï‡∏¥‡∏®‡∏≤‡∏™‡∏ï‡∏£‡πå‡πÅ‡∏•‡∏∞‡∏™‡∏ñ‡∏≤‡∏õ‡∏±‡∏ï‡∏¢‡∏Å‡∏£‡∏£‡∏°', subtitle: '‡∏ß‡∏±‡∏î ‡πÇ‡∏ö‡∏£‡∏≤‡∏ì‡∏™‡∏ñ‡∏≤‡∏ô ‡πÄ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏£‡∏≤‡∏ß‡πÉ‡∏ô‡∏≠‡∏î‡∏µ‡∏ï' }
            };
            
            document.getElementById('headerIcon').textContent = headers[tabName].icon;
            document.getElementById('headerTitle').textContent = headers[tabName].title;
            document.getElementById('headerSubtitle').textContent = headers[tabName].subtitle;
            
            // Add active to clicked button
            event.target.classList.add('active');
            
            // Scroll to top
            window.scrollTo({ top: 0, behavior: 'smooth' });
        }

        // Check URL parameter on load
        window.addEventListener('DOMContentLoaded', function() {
            const urlParams = new URLSearchParams(window.location.search);
            const category = urlParams.get('category');
            if (category && ['tradition', 'craft', 'food', 'history'].includes(category)) {
                // Click the appropriate tab button
                const buttons = document.querySelectorAll('.tab-button');
                const index = ['tradition', 'craft', 'food', 'history'].indexOf(category);
                if (buttons[index]) {
                    buttons[index].click();
                }
            }
        });
    </script>
</body>
</html>
